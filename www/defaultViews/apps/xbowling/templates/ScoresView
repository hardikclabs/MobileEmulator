<script type="text/javascript">
    (function (scoresView) {
    scoresView.nameText = null,
    scoresView.nameTap = null,

    scoresView.load = function () {
        var elm = jQuery("#scores-view-wrapper");
        var scoreGames = jQuery("#scores-game-wrapper", elm);

        var templateElm = elm.parent("[data-template]");
        scoresView.nameText = templateElm.attr("data-scoresview-nametext");
        scoresView.nameTap = templateElm.attr("data-scoresview-nametap");

        var games = window.games();

        for (var i = 0; i < games.length; i++) {
            games[i].getTapAttribute = function (squareScore) {
                return ko.dependentObservable(function () {
                    return squareScore() !== "";
                }, this)();
            }
        }

        ko.applyBindings({ games: window.games }, scoreGames.get(0));

        elm.removeClass("hidden");

        var newHeight = jQuery(document).height() - elm.position().top;
        elm.height(newHeight);

        var mainScroll = new iScroll(elm.get(0), {
            snap: false,
            momentum: true,
            hScroll: false,
            hScrollbar: false,
            vScroll: true,
            vScrollbar: false
        });
        scn.viewEngine.registerViewDestructor(function () {
            if (mainScroll) {
                mainScroll.destroy();
                mainScroll = null;
            }
        });

        var frameWrappers = jQuery(".frame-wrapper", elm);
        var frameWrappersLength = frameWrappers.length;
        for (var i = 0; i < frameWrappersLength; i++) {
            var frameWrapper = jQuery(frameWrappers[i]);
            var scroller = frameWrapper.children().first();
            var frame = jQuery(".frame:first", scroller);

            var singleFrameWidth = frame.outerWidth(true);
            frameWrapper.width(singleFrameWidth * 5);
            scroller.width(singleFrameWidth * 10);

            var scroll = new iScroll(frameWrapper.get(0), {
                snap: true,
                momentum: false,
                hScroll: true,
                hScrollbar: false,
                vScroll: false,
                vScrollbar: false,
                onScrollEnd: function () {
                    var pageIndicator = jQuery(this.wrapper).siblings(".page-indicator");
                    jQuery(".selected", pageIndicator).removeClass("selected");
                    jQuery(":nth-child(" + (this.currPageX + 1) + ")", pageIndicator).addClass("selected");
                }
            });
            scn.viewEngine.registerViewDestructor(function () {
                if (scroll) {
                    scroll.destroy();
                    scroll = null;
                }
            });
        }


        scn.gameplay.wireFrameTaps(elm);

        if (mainScroll) {
            mainScroll.refresh();
        }
    },

    scoresView.getName = function (name) {
        if (scoresView.nameText && scoresView.nameText != "") {
            return scoresView.nameText;
        } else {
            return name;
        }
    },

    scoresView.onNameTap = function () {
        if (scoresView.nameTap) {
            scn.viewEngine.callFunction(this, scoresView.nameTap);
        } else {
            scn.gameplay.onBowlingLaneScoreNameClick(arguments);
        }
    }
}(window.scn.views.scoresView = window.scn.views.scoresView || {}));
</script>
